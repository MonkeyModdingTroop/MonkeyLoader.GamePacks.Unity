<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>MonkeyLoader.GamePacks.Unity</id>
    <version>0.7.0-beta</version>
    <title>Unity GamePack</title>
    <authors>Banane9</authors>
    <owners>Banane9</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This Game Pack for MonkeyLoader provides basic hooks for Unity modding,
      such as when the first scene has been loaded,
      to prevent the dynamic Unity bindings from getting messed up.
    </description>
    <readme>README.md</readme>
    <license type="expression">LGPL-3.0-or-later</license>
    <projectUrl>https://github.com/MonkeyModdingTroop/MonkeyLoader.GamePacks.Unity</projectUrl>
    <repository type="git" url="https://github.com/MonkeyModdingTroop/MonkeyLoader.GamePacks.Unity.git" />
    <tags>mod mods modding mod loader harmony unity game games mono config configuration nuget logging monkeyloader</tags>

    <dependencies>
      <group targetFramework=".NETFramework4.6.2">
        <dependency id="MonkeyLoader" version="0.24.0-beta" />
      </group>
    </dependencies>
  </metadata>

  <files>
    <file
      src="..\README.md"
      target="README.md" />

    <file
      src="bin\Debug\net472\MonkeyLoader.Unity.Integration.dll"
      target="lib\net462\MonkeyLoader.Unity.Integration.dll" />
    <file
      src="bin\Debug\net472\MonkeyLoader.Unity.Integration.xml"
      target="lib\net462\MonkeyLoader.Unity.Integration.xml" />
    <file
      src="bin\Debug\net472\MonkeyLoader.Unity.Integration.pdb"
      target="lib\net462\MonkeyLoader.Unity.Integration.pdb" />
  </files>
</package>